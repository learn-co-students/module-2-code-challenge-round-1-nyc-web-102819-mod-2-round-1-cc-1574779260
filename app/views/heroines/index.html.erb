<h2>All Heroines</h2>

<%= link_to 'Create Heroine', new_heroine_path %>

<%= form_with(url: "/search", method: "get") do %>
  <%= label_tag(:q, "Search for:") %>
  <%= text_field_tag(:q) %>
  <%= submit_tag("Search") %>
<% end %>

<ul>
  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %>
      
    </li>
    <br>
  <% end %>
</ul>
