<h1>Create a new Heroine</h1>
<hr>
<% if flash[:errors] %>
<% flash[:errors].each do |msg| %>
<%= msg %>
<% end%>
<% end%>
<br>
<%= form_for @heroine do |f| %>
<%=f.label :name%>
<%= f.text_area :name %>
<br>
<%= f.label :super_name %>
<%= f.text_area :super_name %>
<br>
<%= f.label :power_id %>
<%= f.collection_select(:power_id, Power.all, :id, :name) %>
<br>
<%= f.submit %>
<% end %>